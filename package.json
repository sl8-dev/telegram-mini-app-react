{
  "name": "telegram-mini-app-react",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "homepage": "https://telegram-mini-apps.github.io/reactjs-template",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "vite --host",
    "deploy": "gh-pages -d dist",
    "format": "prettier ./src --write",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "predeploy": "pnpm run build"
  },
  "dependencies": {
    "@apollo/client": "^3.10.5",
    "@telegram-apps/react-router-integration": "^1.0.1",
    "@telegram-apps/sdk": "^1.1.3",
    "@telegram-apps/sdk-react": "^1.1.3",
    "@telegram-apps/telegram-ui": "^2.1.4",
    "@tonconnect/ui-react": "^2.0.5",
    "crypto-js": "^4.2.0",
    "eruda": "^3.0.1",
    "framer-motion": "^11.2.12",
    "graphql": "^16.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.12.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "gh-pages": "^6.1.1",
    "globals": "^15.2.0",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
